{
  "name": "blossom-back",
  "version": "1.0.0",
  "description": "Blossom backend",
  "main": "server.ts",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "start": "npm run build && node dist/server.js",
    "start:prod": "node dist/server.js",
    "build": "tsc && echo 'Build finished'",
    "dev": "nodemon",
    "lint": "eslint --ext .ts src/",
    "lint:fix": "npm run lint -- --fix",
    "migration:generate": "sequelize migration:generate --name",
    "migration": "sequelize db:migrate",
    "seed:generate": "npx sequelize-cli seed:generate --name",
    "seed:run": "npx sequelize-cli db:seed:all"
  },
  "author": "Daniel Torres",
  "license": "ISC",
  "dependencies": {
    "blossom-back": "file:",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "graphql": "^16.11.0",
    "graphql-http": "^1.22.4",
    "pg": "^8.15.6",
    "pg-hstore": "^2.3.4",
    "ruru": "^2.0.0-beta.22",
    "sequelize": "^6.37.7",
    "sequelize-typescript": "^2.1.6",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tsconfig-paths": "^4.2.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.3",
    "@types/pg": "^8.11.14",
    "@types/sequelize": "^4.28.20",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3",
    "sequelize-cli": "^6.6.2",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.1"
  }
